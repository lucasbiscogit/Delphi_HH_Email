  object ADOFecharCaixa: TADOQuery
    Connection = DMPrincipal.ADOConnectionPrincipal
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'select *  from TB_CAIXA_ABREFECHA where DATA_FINAL is null and H' +
        'ORA_FINAL is null')
